local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))();
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))();
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))();
local Window = Fluent:CreateWindow({Title="TRD GUI",SubTitle="tiktok @christrd",TabWidth=160,Size=UDim2.fromOffset(580, 460),Acrylic=true,Theme="Dark",MinimizeKey=Enum.KeyCode.LeftControl});
local Tabs = {Main=Window:AddTab({Title="Main",Icon=""}),Main=Window:AddTab({Title="scripts",Icon=""}),Settings=Window:AddTab({Title="Settings",Icon="settings"})};
local Options = Fluent.Options;
do
	Fluent:Notify({Title="Notification",Content="follow @christrd (i made this script to get better at scripting so if i make any mistakes contact me)",SubContent="SubContent",Duration=5});
	Tabs.Main:AddParagraph({Title="tiktok @christrd",Content="you can find me here (more socials later on)"});
	local Input = Tabs.scripts:AddInput("Input", {Title="Buy Character",Default="Default",Placeholder="Placeholder",Numeric=false,Finished=false,Callback=function(Value)
		local args = {[1]="Character",[2]=Value};
		game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Buy"):FireServer(unpack(args));
	end});
	Input:OnChanged(function()
		print("Input updated:", Input.Value);
	end);
end
Tabs.scripts:AddButton({Title="Male",Description="can use only on Jury or Sepectator",Callback=function()
	local args = {[1]="Gender",[2]="Male"};
	game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Buy"):FireServer(unpack(args));
end});
Tabs.scripts:AddButton({Title="Female",Description="can use only on Jury or Sepectator",Callback=function()
	local args = {[1]="Gender",[2]="Female"};
	game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Buy"):FireServer(unpack(args));
end});
Tabs.scripts:AddButton({Title="Car Animation Script",Description="this sets all your animations ids to the car id",Callback=function()
	loadstring(game:HttpGet("https://pastebin.com/raw/qYirf4QR", true))();
end});
Tabs.scripts:AddButton({Title="Infinte Yield",Description="funny fe script",Callback=function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/edgeiy/infiniteyield/master/source", true))();
end});
local Slider = Tabs.scripts:AddSlider("Slider", {Title="Animation Speed Changer",Description="Changes your animation speed. 1-15",Default=1,Min=1,Max=15,Rounding=1,Callback=function(Value)
	wait = task.wait;
	local a = game:GetService("Players").LocalPlayer.Character or game:GetService("Players").LocalPlayer.CharacterAdded:wait();
	while wait() do
		local b = a:FindFirstChildOfClass("Humanoid") or a:FindFirstChildOfClass("AnimationController");
		if (not b or not a) then
			continue;
		end
		for c, d in next, b:GetPlayingAnimationTracks() do
			d:AdjustSpeed(Value);
		end
	end
end});
Slider:OnChanged(function(Value)
	print("Slider changed:", Value);
end);
Slider:SetValue(3);
local Slider = Tabs.scripts:AddSlider("Slider", {Title="Walkspeed",Description="Changes your Walkspeed. 1-1000",Default=16,Min=1,Max=1000,Rounding=1,Callback=function(Value)
	game.Players.ChildAdded:Connect(function(plr)
		while true do
			wait();
			if plr.Character then
				break;
			end
		end
		local hum = plr.Character:WaitForChild("Humanoid");
		hum.Walkspeed = Value;
	end);
end});
Slider:OnChanged(function(Value)
	print("Slider changed:", Value);
end);
Slider:SetValue(16);
local Slider = Tabs.scripts:AddSlider("Slider", {Title="Jumppower",Description="Changes your Walkspeed. 1-1000",Default=50,Min=1,Max=1000,Rounding=1,Callback=function(Value)
	game.Players.ChildAdded:Connect(function(plr)
		while true do
			wait();
			if plr.Character then
				break;
			end
		end
		local hum = plr.Character:WaitForChild("Humanoid");
		hum.Jumppower = Value;
	end);
end});
Slider:OnChanged(function(Value)
	print("Slider changed:", Value);
end);
Slider:SetValue(50);
SaveManager:SetLibrary(Fluent);
InterfaceManager:SetLibrary(Fluent);
SaveManager:IgnoreThemeSettings();
SaveManager:SetIgnoreIndexes({});
InterfaceManager:SetFolder("FluentScriptHub");
SaveManager:SetFolder("FluentScriptHub/specific-game");
InterfaceManager:BuildInterfaceSection(Tabs.Settings);
SaveManager:BuildConfigSection(Tabs.Settings);
Window:SelectTab(1);
Fluent:Notify({Title="Fluent",Content="The script has been loaded.",Duration=8});
SaveManager:LoadAutoloadConfig();
